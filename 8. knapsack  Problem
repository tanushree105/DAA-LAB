#include<stdio.h>
int max(int a,int b){
	if(a>b){
		return a;
	}
	else{
		return b;
	}
}

int knapsack(int W, int wt[],int val[],int N){
	int d[N+1][W+1];
	for(int i=0; i<=N; i++){
		for(int j=0; j<=W; j++){
			if(i==0||j==0)
				d[i][j] = 0;
			else if(wt[i-1]<=j)
				d[i][j] = max(val[i-1]+d[i-1][j-wt[i-1]],d[i-1][j]);
			else
				d[i][j] = d[i-1][j];
		}
	}
	return d[N][W];
}

void main(){
	int n,W;
  scanf("%d",&n);
	int val[n],wt[n];
	for(int i=0; i<n; i++){
		scanf("%d",&val[i]);
	}
	for(int i=0; i<n; i++){
		scanf("%d",&wt[i]);
	}
	scanf("%d",&W);
	int r = knapsack(W,wt,val,n);
	printf("%d",r);
}
